{
	"name": "next-project",
	"version": "1.0.0",
	"main": "index.js",
	"license": "UNLICENSED",
	"dependencies": {
		"@apollo/client": "^3.3.7",
		"babel-plugin-module-resolver": "^4.0.0",
		"babel-plugin-styled-components": "1.10.6",
		"cross-env": "^5.2.0",
		"dayjs": "^1.10.4",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"formik": "^2.0.8",
		"graphql": "^15.4.0",
		"mobx": "^4.11.0",
		"mobx-devtools-mst": "^0.9.21",
		"mobx-react": "^6.1.4",
		"mobx-state-tree": "^3.10.2",
		"next": "^9.4.4",
		"next-routes": "^1.4.2",
		"nodemon": "^2.0.2",
		"react": "^16.12.0",
		"react-cookie": "^4.0.3",
		"react-dom": "^16.12.0",
		"react-number-format": "^4.4.1",
		"react-perfect-scrollbar": "1.5.3",
		"reactstrap": "^8.2.0",
		"rimraf": "3.0.0",
		"styled-components": "^4.4.1",
		"ts-node": "^8.1.1",
		"typescript": "^3.7.3",
		"exceljs": "4.1.1"
	},
	"scripts": {
		"####### development mode": "",
		"dev": "cross-env NODE_ENV=development nodemon",
		"build": "next build",
		"start": "next start",
		"clean": "rimraf dist/* .next/* build/* out/*"
	},
	"babel 설정 =========================================================================": {},
	"babel": {
		"presets": [
			[
				"next/babel",
				{
					"preset-env": {
						"corejs": 3,
						"useBuiltIns": "entry"
					}
				}
			]
		],
		"ignore": [
			"./node_modules"
		],
		"compact": false,
		"plugins": [
			[
				"@babel/plugin-proposal-decorators",
				{
					"legacy": true
				}
			],
			[
				"@babel/plugin-proposal-class-properties",
				{
					"loose": true
				}
			],
			[
				"styled-components",
				{
					"ssr": true,
					"displayName": true,
					"preprocess": false
				}
			],
			[
				"module-resolver",
				{
					"root": [
						"."
					],
					"alias": {
						"~/src": "./src",
						"~/gen": "./src/generated",
						"~/gql": "./src/graphql",
						"~/views": "./src/views",
						"~/components": "./src/components",
						"~/containers": "./src/containers",
						"~/utils": "./src/utils",
						"~/public": "./public",
						"~/stores": "./src/stores",
						"~/types": "./src/@types",
						"~/styles": "./src/styles"
					}
				}
			]
		]
	},
	"nodemon 설정 =====================================================================": {},
	"nodemonConfig": {
		"watch": [
			"server/**/*"
		],
		"exec": "ts-node --transpile-only --project tsconfig.server.json server/index.ts",
		"ext": "js ts",
		"ignore": [
			"node_modules",
			".next",
			"public",
			"build",
			"logs",
			"dist"
		]
	},
	"cacheDirectories": [
		"node_modules/",
		".next/cache/"
	],
	"개발쪽 의존성 모듈 =====================================================================": {},
	"devDependencies": {
		"@babel/plugin-proposal-class-properties": "^7.7.4",
		"@babel/plugin-proposal-decorators": "^7.7.4",
		"@graphql-codegen/cli": "^1.20.1",
		"@graphql-codegen/typescript": "^1.20.1",
		"@graphql-codegen/typescript-operations": "^1.17.14",
		"@graphql-codegen/typescript-react-apollo": "^2.2.1",
		"@types/node": "^12.12.14",
		"@types/react": "^16.9.13"
	}
}
