{
	"name": "next-project",
	"version": "1.0.0",
	"main": "index.js",
	"license": "MIT",
	"dependencies": {
		"@babel/core": "^7.13.8",
		"@babel/plugin-proposal-class-properties": "^7.13.0",
		"@babel/plugin-proposal-decorators": "^7.13.5",
		"babel-plugin-module-resolver": "^4.1.0",
		"cross-env": "^7.0.3",
		"dayjs": "^1.10.4",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"formik": "^2.2.6",
		"mobx": "^6.1.8",
		"mobx-state-tree": "^5.0.1",
		"module-resolver": "^1.0.0",
		"next": "^10.0.8",
		"next-routes": "^1.4.2",
		"nodemon": "^2.0.7",
		"react": "^17.0.1",
		"react-cookie": "^4.0.3",
		"react-dom": "^17.0.1",
		"react-number-format": "^4.4.4",
		"reactstrap": "^8.9.0",
		"styled-components": "^5.2.1",
		"ts-node": "^9.1.1",
		"typescript": "^4.2.3"
	},
	"scripts": {
		"####### development mode": "",
		"dev": "cross-env NODE_ENV=development nodemon",
		"build": "next build",
		"start": "next start"
	},
	"babel 설정 =========================================================================": {},
	"babel": {
		"presets": [
			[
				"next/babel",
				{
					"preset-env": {
						"corejs": 3,
						"useBuiltIns": "entry"
					}
				}
			]
		],
		"ignore": [
			"./node_modules"
		],
		"compact": false,
		"plugins": [
			[
				"@babel/plugin-proposal-decorators",
				{
					"legacy": true
				}
			],
			[
				"@babel/plugin-proposal-class-properties",
				{
					"loose": true
				}
			],
			[
				"styled-components",
				{
					"ssr": true,
					"displayName": true,
					"preprocess": false
				}
			],
			[
				"module-resolver",
				{
					"root": [
						"."
					],
					"alias": {
						"~/src": "./src",
						"~/gen": "./src/generated",
						"~/gql": "./src/graphql",
						"~/views": "./src/views",
						"~/components": "./src/components",
						"~/containers": "./src/containers",
						"~/utils": "./src/utils",
						"~/public": "./public",
						"~/stores": "./src/stores",
						"~/types": "./src/@types",
						"~/styles": "./src/styles"
					}
				}
			]
		]
	},
	"nodemon 설정 =====================================================================": {},
	"nodemonConfig": {
		"watch": [
			"server/**/*"
		],
		"exec": "ts-node --transpile-only --project tsconfig.server.json server/index.ts",
		"ext": "js ts",
		"ignore": [
			"node_modules",
			".next",
			"public",
			"build",
			"logs",
			"dist"
		]
	},
	"cacheDirectories": [
		"node_modules/",
		".next/cache/"
	],
	"devDependencies": {
		"@types/node": "^14.14.32",
		"@types/react": "^17.0.2"
	}
}
